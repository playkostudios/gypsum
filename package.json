{
  "name": "manifold-wle",
  "version": "1.0.0",
  "description": "Helper library for using Manifold with Wonderland Engine",
  "exports": "./dist/index.esm.min.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist/*.js dist/*.map dist/*.ts",
    "build": "rimraf dist/*.js dist/*.map && esbuild src/index.ts --sourcemap --bundle --minify --platform=browser --external:@wonderlandengine/api --outfile=dist/index.esm.min.js",
    "gen-types": "npm-dts generate --entry index.ts --output dist/index.d.ts -L debug",
    "prepack": "npm run clean && npm run gen-types && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playkostudios/manifold-wle.git"
  },
  "keywords": [
    "3D",
    "mesh",
    "manifold",
    "geometry",
    "solid",
    "CAD",
    "triangle",
    "SDF",
    "CSG",
    "wonderland",
    "typescript"
  ],
  "contributors": [
    {
      "name": "Playko",
      "url": "https://www.playko.com/"
    },
    {
      "name": "rafern",
      "email": "rafael@playko.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/playkostudios/manifold-wle/issues"
  },
  "homepage": "https://github.com/playkostudios/manifold-wle#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@wonderlandengine/api": "^0.9.0",
    "esbuild": "^0.15.12",
    "manifold-3d": "^1.0.0",
    "npm-dts": "^1.3.12",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}
